{
  "name": "@r0b0t3d/react-native-carousel",
  "title": "React Native Carousel",
  "version": "1.0.4",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "deploy": "cp -R lib example/node_modules/@r0b0t3d/react-native-carousel",
    "build": "rm -rf lib && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src/**/*.ts",
    "fix-code": "prettier-eslint --write 'src/**/*.{ts,tsx}' ",
    "test": "jest --config jestconfig.json --passWithNoTests",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish": "npm run prepare && npm run prepublishOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r0b0t3d/react-native-carousel",
    "baseUrl": "https://github.com/r0b0t3d/react-native-carousel"
  },
  "keywords": [
    "react-native"
  ],
  "author": {
    "name": "Tuan Luong",
    "email": "tuanluong.it@gmail.com"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "dependencies": {
    "@r0b0t3d/react-native-hooks": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.1",
    "react-native": ">=0.59.0-rc.0 <1.0.x",
    "react-native-reanimated": "^1.7.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/react": "^16.9.25",
    "@types/react-native": "^0.61.23",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.8.3",
    "react-native": "^0.59.10",
    "react-native-reanimated": "^1.7.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "files": [
    "lib/**/*"
  ]
}
